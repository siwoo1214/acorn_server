package day06Prac;

public class Acorn {
	String id;
	int pw;
	String name;
	
	public Acorn(String id, String pw, String name) {
		this.id = id;
		this.pw = pw;
		this.name = name;
	}
	
	public Acorn() {
		// TODO Auto-generated constructor stub
	}

	public String getId() {
		return id;
	}

	public int getPw() {
		return pw;
	}

	public String getName() {
		return name;
	}

	@Override
	public String toString() {
		return "Acorn [id=" + id + ", pw=" + pw + ", name=" + name + "]";
	}
	
}
